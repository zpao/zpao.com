---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

   <title>Paul O’Shannessy (mozilla)</title>
   <link href="{{ site.url }}/feeds/mozilla.xml" rel="self" />
   <link href="{{ site.url }}" />
   <updated>{{ site.time | date_to_xmlschema }}</updated>
   <id>{{ site.url }}/feeds/mozilla</id>
   <author>
     <name>Paul O’Shannessy</name>
   </author>

{% for post in site.posts %}
{% if post.tags contains "Mozilla" or post.tags contains "mozilla" %}
  <entry>
    <title type="html">{{ post.title | markdownify2 | xml_escape }}</title>
    <link href="{{ site.url }}{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>{{ site.url }}{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
{% endif %}
{% endfor %}

</feed>

